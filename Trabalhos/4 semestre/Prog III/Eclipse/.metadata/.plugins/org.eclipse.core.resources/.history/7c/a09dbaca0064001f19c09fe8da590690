
public class Veiculo {

	private String modelo;
	private String cor;
	private String ano;
	
	public void setModelo(String modelo) {
		this.modelo = modelo;
	}
	public void setCor(String cor) {
		this.cor = cor;
	}
	public void setAno(String ano) {
		this.ano = ano;
	}
	
	public String getModelo() {
		return modelo;
	}
	public String getCor() {
		return cor;
	}
	public String getAno() {
		return ano;
	}
}
