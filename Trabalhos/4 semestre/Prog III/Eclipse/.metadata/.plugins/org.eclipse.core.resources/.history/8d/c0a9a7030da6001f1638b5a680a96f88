package entity;

public class Vehicle {

	private int id;
	private String marca;
	private String modelo;
	private int ano;
	private String cor;
	private double quilometragem;
	private String estado;
	private String combustivel;
	
	
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getMarca() {
		return marca;
	}
	public void setMarca(String marca) {
		this.marca = marca;
	}
	public String getModelo() {
		return modelo;
	}
	public void setModelo(String modelo) {
		this.modelo = modelo;
	}
	public int getAno() {
		return ano;
	}
	public void setAno(int ano) {
		this.ano = ano;
	}
	
	public Vehicle(int id, String marca, String modelo, int ano) {
		super();
		this.id = id;
		this.marca = marca;
		this.modelo = modelo;
		this.ano = ano;
	}
	
	@Override
	public String toString() {
		return "Vehicle: \nid: "+id+"\nmarca: " + marca + " \nmodelo: " + modelo + " \nano: " + ano + "\n";
	}
	
	
}
