
public class calculadora {

	private int num1;
	private int num2;
	private String sinal;
	
	public void setNum1(int num1) {
		this.num1 = num1;
	}
	public void setNum2(int num2) {
		this.num2 = num2;
	}
	public void setSinal(String sinal) {
		this.sinal = sinal;
	}
	
	public int getNum1() {
		return num1;
	}
	public int getNum2() {
		return num2;
	}
	public String getSinal() {
		return sinal;
	}
	
}
